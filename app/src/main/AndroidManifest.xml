<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.codebot.shapes">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="StoreFront"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OwnerView"></activity>
        <activity android:name=".Search" />
        <activity android:name=".OwnerLogin" />
        <activity android:name=".ProdAddActivity" />
        <activity android:name=".PathActivity" />
        <activity android:name=".ProductListActivity"/>
        <activity android:name=".GridActivity"/>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name="HttpConnector"
            >
        </service>
    </application>

</manifest>